{
    "manifest_version": 3,
    "name": "Clear youtubes Cache",
    "version": "1.0",
    "description": "An extension to clear youtubes cache whenever a new video is played.",
    "permissions": [
      "activeTab",
      "browsingData",
      "webNavigation",
      "scripting"
    ],
    "host_permissions": [
      "*://*.youtube.com/*"
    ],
    "background": {
      "service_worker": "scripts/background.js"
    },
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["scripts/content.js"]
      }
    ],
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": {
      }
    },
    "icons": {
    }
}
  